.button-area-position button:focus, .button-area-position button:active{
  box-shadow: none;
  outline:none;
}
.button-area-position button {
  background-color: transparent;
  -webkit-border-radius: 10px;
  border-radius: 10px;
  border: none;
  color: #FFFFFF;
  cursor: pointer;
  display: inline-block;
  font-family: Arial;
  font-size: 20px;
  padding: 5px 10px;
  text-align: center;
  text-decoration: none;
  -webkit-animation: glowing 1500ms infinite;
  -moz-animation: glowing 1500ms infinite;
  -o-animation: glowing 1500ms infinite;
  animation: glowing 1500ms infinite;
}
@-webkit-keyframes glowing {
  0% { background-color: transparent; -webkit-box-shadow: 0 0 3px #000; }
  50% { background-color: transparent; -webkit-box-shadow: 0 0 10px #000; }
  100% { background-color: transparent; -webkit-box-shadow: 0 0 3px #000; }
}

@-moz-keyframes glowing {
  0% { background-color: transparent; -moz-box-shadow: 0 0 3px #000; }
  50% { background-color: transparent; -moz-box-shadow: 0 0 10px #000; }
  100% { background-color: transparent; -moz-box-shadow: 0 0 3px #000; }
}

@-o-keyframes glowing {
  0% { background-color: transparent; box-shadow: 0 0 3px #000; }
  50% { background-color: transparent; box-shadow: 0 0 10px #000; }
  100% { background-color: transparent; box-shadow: 0 0 3px #000; }
}

@keyframes glowing {
  0% { background-color: transparent; box-shadow: 0 0 3px #000; }
  50% { background-color: transparent; box-shadow: 0 0 10px #000; }
  100% { background-color: transparent; box-shadow: 0 0 3px #000; }
}

